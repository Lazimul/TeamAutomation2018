<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Target Shopping Website " parallel="false">
    <listeners>
        <listener class-name="Listeners"/>
    </listeners>

    <parameter name="OS" value="macOS Mojave"/>
    <parameter name="browser" value="chrome"/>
    <parameter name="browserVersion" value="71.0.3578.98"/>
    <parameter name="url" value="https://www.target.com"/>

    <test name = "Target Home">
        <classes>
            <!--<class name="TestSearch"/>-->
            <class name="ValidateTitle"/>


        </classes>
    </test>
</suite>